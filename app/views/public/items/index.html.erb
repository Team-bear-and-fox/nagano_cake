<h1>商品一覧</h1>
<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
<%= @items.each do |item| %>
  <tbody>
    <td><%= item.id %></td>
    <td><%= item.name %></td>
    <td><%= item.value %></td>
    <td><%= item.genre_id %></td>
    <td><%= item.status %></td>
  </tbody>
<% end %>
</table>